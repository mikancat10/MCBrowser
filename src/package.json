{
  "name": "mcbrowser",
  "version": "10.0.0",
  "description": "MCBrowser",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
"devDependencies": {
  "electron": "^40.1.0",
  "electron-builder": "^26.4.0",
  "electron-reload": "^1.5.0"
},
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.34.0",
    "axios": "^1.13.4",
    "cross-fetch": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3"
  },
"build": {
  "appId": "com.mikan.mcbrowser",
  "productName": "MCBrowser",

  "directories": {
    "output": "dist"
  },

  "files": [
    "main.js",
    "preload.js",
    "renderer.js",
    "*.html",
    "*.css",
    "assets/**/*",
    "node_modules/**/*"
  ],

  "asar": true,

  "win": {
    "target": "nsis",
    "icon": "assets/icon.ico"
  },

  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  },

  "publish": [
    {
      "provider": "github",
      "owner": "mikancat10",
      "repo": "MCBrowser"
    }
  ]
}

}

